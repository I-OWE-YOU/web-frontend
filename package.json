{
  "name": "tegoedje",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:prod": "vue-cli-service build --mode production",
    "build:stage": "vue-cli-service build --mode staging",
    "lint": "run-s lint:all:*",
    "deploy:prod": "vue-cli-service s3-deploy --mode production",
    "deploy:stage": "vue-cli-service s3-deploy --mode staging",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss}\"",
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:stylelint": "stylelint --fix",
    "test": "vue-cli-service build --mode test"
  },
  "dependencies": {
    "@stripe/stripe-js": "^1.3.0",
    "aws-amplify": "^2.2.7",
    "aws-amplify-vue": "^1.1.4",
    "axios": "^0.21.1",
    "bootstrap": "^4.4.1",
    "google-maps-api-loader": "^1.1.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.3",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-standard": "5.1.x",
    "@web-types/vue": "^2.5.0-3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "7.0.x",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "2.20.x",
    "eslint-plugin-node": "11.0.x",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "4.2.x",
    "eslint-plugin-standard": "4.0.x",
    "eslint-plugin-vue": "^6.1.2",
    "hard-source-webpack-plugin": "^0.13.1",
    "hygen": "4.0.x",
    "imagemin-lint-staged": "^0.4.0",
    "lint-staged": "10.0.x",
    "markdownlint-cli": "0.22.x",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "stylelint": "13.2.x",
    "stylelint-config-css-modules": "2.2.x",
    "stylelint-config-prettier": "8.0.x",
    "stylelint-config-recess-order": "2.0.x",
    "stylelint-config-standard": "20.0.x",
    "stylelint-scss": "3.14.x",
    "vue-cli-plugin-s3-deploy": "^4.0.0-rc3",
    "vue-template-compiler": "^2.6.11"
  },
  "engines": {
    "node": ">=10.13.3",
    "yarn": ">=1.0.0"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  }
}
